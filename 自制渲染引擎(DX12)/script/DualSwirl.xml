<psystem name="DualSwirl">
	<blendop>BLENDOP_ADD</blendop>
	<srcblend>BLEND_SRCALPHA</srcblend>
	<dstblend>BLEND_INVSRCALPHA</dstblend>
	<numsubsets>2</numsubsets>
	<subset name="Swirl_0">
		<psize>8,8</psize>
		<emitrate>0.6</emitrate>
		<numparticles>30</numparticles>
		<texture>Particle.dds</texture>
		<events count="8">
			<init type="life">45</init>
			<init type="color">rand(0,1),rand(0,1),rand(0,1),1</init>
			<init type="veloc">0,0,0</init>
			<fade time="10" type="veloc">-10,-10,0</fade>
			<fade time="20" type="veloc">-10,10,0</fade>
			<fade time="30" type="veloc">10,10,0</fade>
			<fade time="40" type="veloc">10,-10,0</fade>
		</events>
	</subset>
	<subset name="Swirl_1">
		<psize>8,8</psize>
		<emitrate>0.6</emitrate>
		<numparticles>30</numparticles>
		<texture>Particle.dds</texture>
		<events count="8">
			<init type="life">45</init>
			<init type="color">rand(0,1),rand(0,1),rand(0,1),1</init>
			<init type="veloc">0,0,0</init>
			<fade time="10" type="veloc">10,-10,0</fade>
			<fade time="20" type="veloc">10,10,0</fade>
			<fade time="30" type="veloc">-10,10,0</fade>
			<fade time="40" type="veloc">-10,-10,0</fade>
		</events>
	</subset>
</psystem>
